<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="2021-09-20" author="Ivan">
        <createTable tableName="players">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="text"/>
            <column name="points" type="int"/>
            <column name="wins_nr" type="int"/>
            <column name="moves_nr" type="int"/>
            <column name="used_cards" type="int"/>
            <column name="games_id" type="int"/>
            <column name="last_asking" type="int"/>
        </createTable>
    </changeSet>

</databaseChangeLog>

